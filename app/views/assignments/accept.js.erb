$('#case-<%= @case.id %>')
  .find('.js-take-case-on-container .container-actions').html(
    "<%= escape_javascript(
           render partial: "cases/take_case_on_message",
                  locals: {
                            kase: @case,
                            success: @success,
                            message: @message,
                          }
         ) %>")
  .end()
  .filter('tr')
  .children()
  .toggleClass('ajax-success');

