= form_for kase, url: cases_path do |f|
  = hidden_field_tag 'correspondence_type', @correspondence_type_abbreviation
  = f.hidden_field :type, value: 'case/sar'

  = render partial: 'cases/sar/new_form_common', locals: { f: f }
  = render partial: 'cases/sar/new_form_details', locals: { f: f }

  = f.submit t('button.next_page_assign'), {class: 'button'}
