$('#case-<%= @case.id %>')
  .find('.js-take-case-on-container .container-actions').html(
    "<%= escape_javascript(
           render partial: 'de_escalated',
                  locals: { kase: @case }
         ) %>")
  .end()
  .filter('tr')
  .children()
  .toggleClass('ajax-success--de-escalate');

