$('#case-<%= @case.id %>')
  .find('.js-take-case-on-container .container-actions').html(
    "<%= escape_javascript(
           render partial: 'take_case_on_or_de_escalate',
                  locals: { case_details: @case }
         ) %>")
  .end()
  .filter('tr')
  .children()
  .toggleClass('ajax-success--de-escalate');

